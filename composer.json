{
    "name": "restlytics/laravel",
    "description": "Adds restlytics in your Laravel application",
    "keywords": ["restlytics", "laravel"],
    "version": "1.0",
    "authors": [
        {
            "name": "Restlytics",
            "email": "info@restlytics.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.3.x|5.4.x|5.5.x",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Restlytics\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Restlytics\\ServiceProvider"
            ]
        }
    }
}
